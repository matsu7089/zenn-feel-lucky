{
  "name": "Zenn Feel Lucky",
  "description": "絵文字からZennの記事をみつける拡張機能",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_popup": "pages/popup.html"
  },
  "permissions": [
    "https://api.zenn.dev/*"
  ]
}